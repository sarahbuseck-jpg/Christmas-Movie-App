<%- include('../partials/header', { title: 'Productions' }) %>

<h2>Production Companies</h2>

<table class="table table-striped">
    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>

    <% productions.forEach(p => { %>
    <tr>
        <td><%= p.production_id %></td>
        <td><%= p.name %></td>
    </tr>
    <% }) %>
</table>

<%- include('../partials/footer') %>